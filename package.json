{
  "name": "team-creator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development next dev",
    "build": "cross-env NODE_ENV=production next build",
    "start": "cross-env NODE_ENV=production next start",
    "lint": "next lint",
    "clean": "rd /s /q .next 2>nul & rd /s /q out 2>nul",
    "deploy": "npm run clean && npm run build && node create-nojekyll.js && npx gh-pages -d out --dotfiles"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.16",
    "next": "^16.0.10",
    "react": "latest",
    "react-dom": "latest",
    "react-transition-group": "^4.4.5",
    "team-creator": "file:",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "autoprefixer": "latest",
    "cross-env": "^10.1.0",
    "eslint": "latest",
    "eslint-config-next": "latest",
    "eslint-plugin-react": "latest",
    "gh-pages": "latest",
    "postcss": "latest",
    "tailwindcss": "latest",
    "touch-cli": "^0.0.1"
  },
  "eslintConfig": {
    "extends": [
      "standard"
    ]
  },
  "homepage": "https://hoserpa.github.io/teamgenerator"
}
